{
  "manifest_version": 3,
  "name": "DoTask Tracker",
  "version": "1.0",
  "description": "Tracks website visits and reports to DoTask app",
  "permissions": [
    "tabs",
    "storage",
    "webNavigation"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}
